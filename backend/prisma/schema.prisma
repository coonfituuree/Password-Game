datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int     @id @default(autoincrement())
  telegramId Int    @unique
  username  String?
  photoUrl  String?
  pts       String?
  rating    String?
  createdAt DateTime @default(now())
}
